  "scripts": {
    "android": "npx react-native run-android --mode=developmentDebug --appId=com.alcohol_tracker.dev --active-arch-only",
    "ios": "npx react-native run-ios --simulator=\"iPhone 16 Pro\" --scheme \"Kiroku (development)\" --mode=\"DebugDevelopment\"",
    "ios:pod:install": "cd ios && pod install && cd ..",
    "ios:pod:reset": "./scripts/podReset.sh",
    "ios-build": "fastlane ios build",
    "android-build": "fastlane android build",
    "startAndroidEmulator": "scripts/start-android.sh",
    "postinstall": "scripts/postInstall.sh",
    "docs": "cd docs && bundle exec jekyll serve --livereload",
    "gradle-clean": "cd android && ./gradlew clean && cd ..",
    "lint": "eslint . --max-warnings=0 --cache --cache-location=node_modules/.cache/eslint",
    "lint:changed": "eslint --fix $(git diff --diff-filter=AM --name-only master -- \"*.js\" \"*.ts\" \"*.tsx\")",
    "lint:watch": "npx eslint-watch --watch --changed",
    "lint:quiet": "eslint . --max-warnings=0 --quiet --cache --cache-location=node_modules/.cache/eslint",
    "gh-actions-build": "./.github/scripts/buildActions.sh",
    "gh-actions-validate": "./.github/scripts/validateActionsAndWorkflows.sh",
    "merge": "./scripts/mergeAndPush.sh",
    "pull-all": "./scripts/pullAll.sh",
    "prettier": "prettier --write .",
    "prettier-watch": "onchange \"**/*.{js,ts,tsx}\" -- prettier --write --ignore-unknown {{changed}}",
    "start": "react-native start",
    "symbolicate:android": "npx metro-symbolicate android/app/build/generated/sourcemaps/react/release/index.android.bundle.map",
    "symbolicate:ios": "npx metro-symbolicate main.jsbundle.map",
    "symbolicate-release:ios": "scripts/release-profile.ts --platform=ios",
    "symbolicate-release:android": "scripts/release-profile.ts --platform=android",
    "jsbundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "encrypt": "./scripts/encrypt.sh",
    "test": "TZ=utc jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:emulators": "USE_EMULATORS=true firebase emulators:exec --only auth,database,storage 'npm test __tests__/integration/emulators'",
    "emulators": "firebase emulators:start",
    "emulators-with-setup": "./scripts/setupEmulators.sh",
    "transpile": "npx tsc --build functions",
    "deploy-functions": "firebase deploy --only functions --debug",
    "workflow-test": "./workflow_tests/scripts/runWorkflowTests.sh",
    "bump": "./scripts/bumpVersion.sh"
  },
